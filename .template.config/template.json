{
    "$schema": "http://json.schemastore.org/template",
    "author": "Unreal852",
    "classifications": ["Console"],
    "identity": "Unreal.Template.Console",
    "name": "Console",
    "shortname": "console",
    "sourceName": "Unreal.Template.Console",
    "tags":
    {
        "language": "C#",
        "type": "project",
    },
    "preferNameDirectory": true,
    "symbols":
    {
        "Framework":
        {
            "type": "parameter",
            "description": "The target framework for the project.",
            "datatype": "choice",
            "choices":
            [
                {
                    "choice": "net6.0",
                },
                {
                    "choice": "net7.0",
                }
            ],
            "defaultValue": "net7.0",
            "replaces": "{TargetFramework}"
        }
    }
}